{
  "name": "@hmeqo/easymodel",
  "version": "0.1.0-beta.3",
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "description": "Easily define your data models use decorator.",
  "type": "module",
  "author": "hmeqo",
  "license": "MIT",
  "keywords": [
    "data",
    "model"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hmeqo/easymodel.git"
  },
  "bugs": {
    "url": "https://github.com/hmeqo/easymodel/issues"
  },
  "homepage": "https://github.com/hmeqo/easymodel#readme",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "require": "./dist/utils/index.cjs",
      "default": "./dist/utils/index.js"
    },
    "./errors": {
      "types": "./dist/errors/index.d.ts",
      "require": "./dist/errors/index.cjs",
      "default": "./dist/errors/index.js"
    },
    "./form": {
      "types": "./dist/form/index.d.ts",
      "require": "./dist/form/index.cjs",
      "default": "./dist/form/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "utils": [
        "./dist/utils/index.d.ts"
      ],
      "errors": [
        "./dist/errors/index.d.ts"
      ],
      "form": [
        "./dist/form/index.d.ts"
      ]
    }
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsup src/index.ts src/errors/index.ts src/utils/index.ts --dts --format esm,cjs",
    "test": "jest",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:fix": "eslint . --fix",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier src --check",
    "format": "prettier --write src"
  },
  "dependencies": {
    "date-fns-tz": "^3.1.3",
    "decimal.js": "^10.4.3",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.8.0",
    "@swc/core": "^1.7.6",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20.14.14",
    "@types/string-format": "^2.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.8.0",
    "jest": "^29.7.0",
    "nuxt": "^3.16.0",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.18.0"
  }
}
